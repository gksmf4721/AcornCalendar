DROP TABLE IF EXISTS JH_MEMBER;
CREATE TABLE `JH_MEMBER`(
    `M_SEQ` BIGINT(20) NOT NULL AUTO_INCREMENT COMMENT '멤버_시퀀스',
    `M_NAME` VARCHAR(20) NOT NULL COMMENT '이름',
    `M_ID` VARCHAR(20) NOT NULL COMMENT '아이디',
    `M_NICKNAME` VARCHAR(30) NOT NULL COMMENT '닉네임',
    `M_EMAIL` VARCHAR(30) NOT NULL COMMENT '이메일',
    `M_BIRTH` VARCHAR(6) NOT NULL COMMENT '생년월일',
    `M_BIRTH_YN` CHAR(1) DEFAULT 'Y' COMMENT '생일_공개_여부',
    `M_DEL_YN` CHAR(1) DEFAULT 'N' COMMENT '회원_삭제_여부',
    PRIMARY KEY(M_SEQ)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='회원';

